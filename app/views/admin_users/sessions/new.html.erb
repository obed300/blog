<%= stylesheet_link_tag 'session_login' %>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.email_field :email, autofocus: true, placeholder: 'Username' %>
  </div>

  <div class="field">
    <%= f.password_field :password, placeholder:'Password', autocomplete: "current-password" %>
  </div>

  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>

<%= render "admin_users/shared/links" %>
