<%= stylesheet_link_tag 'session_login' %>

<%= form_for(resource, as: resource_name, url: session_path(resource_name), html: { class: 'bodies' }) do |f| %>
  <div class="field">
    <%= f.email_field :email, autofocus: true, placeholder: 'Username',class: 'inputs' %>
  </div>

  <div class="field">
    <%= f.password_field :password, placeholder:'Password', autocomplete: "current-password", class: 'inputs' %>
  </div>

  <div class="field">
    <%= f.submit "Log in", class: 'inputs' %>
  </div>
<% end %>


<%= render "users/shared/links" %>
